@model List<User>

<div>
  @await Html.PartialAsync("_NavigationBar")
</div>

<br>

<h1 class="display-4 text-center">All Users</h1>

<div class="container text-center">
  <div class="row  bg-success">
    <h1 class="col">User#</h1>
    <h1 class="col">Name</h1>
  </div>

  @foreach(User person in Model)
  {
  <div class="row border border-5 border-secondary-color">
    <h1 class="col">@person.UserId</h1>
    <h1 class="col">@person.FirstName @person.LastName</h1>
  </div>
  }
</div>


